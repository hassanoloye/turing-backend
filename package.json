{
  "name": "turing-backend",
  "version": "1.0.0",
  "description": "Backend for Turing e-commerce system",
  "main": "src/app.ts",
  "engineStrict": true,
  "engines": {
    "node": ">=10.15.1"
  },
  "scripts": {
    "build": "tsc",
    "prewatch": "npm run versions-check",
    "dev": "nodemon -r dotenv/config dist/app.js",
    "watch": "tsc --watch",
    "test": "mocha -r ts-node/register --file ./tests/setup.ts tests/**/*.test.ts --exit",
    "versions-check": "node -r dotenv/config scripts/check_versions.js",
    "config:procedure": "node -r dotenv/config scripts/generate_mysql_procedure_config",
    "config:table": "node -r dotenv/config scripts/generate_mysql_table_config",
    "db:execute": "node scripts/execute_db_query",
    "db:test:execute": "NODE_ENV=test node scripts/execute_db_query",
    "preinstall": "npm run versions-check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hassanoloye/turing-backend.git"
  },
  "author": "Hassan Oyeboade <oyeboadehassan@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hassanoloye/turing-backend/issues"
  },
  "homepage": "https://github.com/hassanoloye/turing-backend#readme",
  "dependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/faker": "^4.1.5",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "http-errors": "^1.7.3",
    "json-refs": "^3.0.13",
    "jsonwebtoken": "^8.5.1",
    "lx-valid": "^1.2.4",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.5",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "require-all": "^3.0.0",
    "semver": "^6.1.1",
    "sequelize": "^5.8.12",
    "stripe": "^7.4.0",
    "swagger-ui-express": "^4.0.7",
    "uuid": "^3.3.2",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.0",
    "@types/http-errors": "^1.6.1",
    "@types/jest": "^24.0.15",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mocha": "^5.2.7",
    "@types/morgan": "^1.7.35",
    "@types/node": "^12.0.10",
    "@types/passport": "^1.0.0",
    "@types/passport-local": "^1.0.33",
    "@types/stripe": "^6.31.4",
    "@types/supertest": "^2.0.7",
    "@types/swagger-ui-express": "^3.0.1",
    "@types/uuid": "^3.4.5",
    "@types/yamljs": "^0.2.30",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "sequelize-typescript": "^0.6.11",
    "supertest": "^4.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2"
  }
}
